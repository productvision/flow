{
  "name": "Module",
  "plural": "modules",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "label": {
      "type": "string",
      "required": true
    },
    "view": {
      "type": "string",
      "required": true
    },
    "controller": {
      "type": "string"
    },
    "model": {
      "type": "string"
    },
    "config": {
      "type": "object"
    }
  },
  "validations": [],
  "relations": {
    "spaces": {
      "type": "hasMany",
      "model": "Space",
      "foreignKey": "moduleId",
      "through": "SpaceModule"
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}

{
  "name": "Space",
  "plural": "spaces",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "configs": {
      "type": "hasMany",
      "model": "Config",
      "foreignKey": "spaceId",
      "through": "SpaceConfig"
    },
    "modules": {
      "type": "hasMany",
      "model": "Module",
      "foreignKey": "spaceId",
      "through": "SpaceModuleConfig"
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
